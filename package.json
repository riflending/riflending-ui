    "prettier:autofix": "yarn prettier --write"
  },
  "dependencies": {
    "@mdi/font": "^3.6.95",
    "@rsksmart/rlogin": "^0.1.1",
    "@sentry/tracing": "^6.0.2",
    "@sentry/vue": "^6.0.2",
    "@walletconnect/web3-provider": "^1.3.1",
    "bignumber.js": "^9.0.1",
    "core-js": "^3.6.4",
    "decimal.js-light": "^2.5.1",
    "ethers": "^5.0.24",
    "lodash": "^4.17.15",
    "register-service-worker": "^1.7.1",
    "roboto-fontface": "*",
    "vue": "^2.6.11",
    "vue-fragment": "^1.5.1",
    "vue-google-charts": "^0.3.3",
    "vue-gtag": "^1.10.0",
    "vue-router": "^3.1.6",
    "vuetify": "^2.2.11",
    "vuex": "^3.1.3"
