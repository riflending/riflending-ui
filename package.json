{
  "name": "rlending-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "npx vue-cli-service serve",
    "build": "npx vue-cli-service build",
    "e2e-chrome": "clear && node_modules/.bin/nightwatch /test-browser/tests --config nightwatch.conf.js --env chrome",
    "selenium": "node_modules/.bin/selenium-standalone install && node_modules/.bin/selenium-standalone start",
    "lint:check": "eslint './src/**/*.{js,vue}'",
    "lint:autofix": "yarn lint:check --fix",
    "prettier": "prettier './src/**/*.{js,vue}'",
